<UserControl x:Class="TypeManager.View.ParaModify.TestControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TypeManager.View.ParaModify"
        mc:Ignorable="d" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel>
                <ComboBox x:Name="cmbTest" IsEditable="True" MinWidth="120" HorizontalAlignment="Left" TextBoxBase.TextChanged="ComboBox_TextChanged"/>
            </StackPanel>
        </DockPanel>
    </Grid>
</UserControl>
